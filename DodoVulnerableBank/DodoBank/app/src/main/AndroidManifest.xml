<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.cysecurity.example.dodobank" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dodobank_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity" >
        </activity>
        <activity
            android:name=".UserHome"
            android:exported="true"
            android:label="@string/title_activity_user_home" >
        </activity>
        <activity
            android:name=".Statement"
            android:label="@string/title_activity_statement" >
        </activity>
        <activity
            android:name=".TransferViewActivity"
            android:label="@string/title_activity_transaction" >
        </activity>
        <activity
            android:name=".DoTransfer"
            android:exported="true"
            android:label="@string/title_activity_do_transfer" >
        </activity>

        <activity
            android:name=".PasswordActivity"
            android:exported="true"
            android:label="@string/title_activity_password" >
        </activity>
    </application>

</manifest>
